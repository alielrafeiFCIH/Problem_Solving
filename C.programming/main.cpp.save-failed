#include <bits/stdc++.h>

using namespace std;

int main()
{
    freopen("fun.in" , "r" , stdin);
    int n,x;
    scanf("%d",&n);
    for(int i =0;i<n;i++){
        cin>>x;
        int sum = 0;
        for(int i = 1;i<=x;i++){
            if(i%2!=0)sum+=i;
        }
        cout<<sum<<endl;
    }
    return 0;
}
