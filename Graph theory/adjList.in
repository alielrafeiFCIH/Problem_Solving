#include <bits/stdc++.h>
using namespace std;

const int N = 105;
vector<pair<int,int> > adj[N];
int n , m;

int main()
{
    scanf("%d%d",&n,&m);
    int a , b , w;
    for(int i = 0 ; i < m ; i++){
        scanf("%d%d",&a,&b);
        adj[a].push_back(make_pair(b,w));
        //adj[b].push_back(a);
    }

    return 0;
}

